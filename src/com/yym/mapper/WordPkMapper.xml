<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yym.dao.WordPkDao">
	<update id="updBank">
		update player set bank=#{bank} where uid=#{uid}
	</update>
	<select id="selPlayer" parameterType="int" resultType="com.yym.entity.Player" >
		select * from player where uid=#{uid}
	</select>
</mapper>